{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": [
    "vue"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "class-methods-use-this": "off",
    "no-prototype-builtins": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "prefer-rest-params": "off",
    "no-await-in-loop": "off",
    "no-restricted-syntax": "off",
    "no-continue": "off",
    "max-len": ["error", { "code": 120 }],
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true
      }
    ],
    "spaced-comment": "off"
  },
  "overrides": [
    {
      "files": ["bin/**/*.js"],
      "rules": {
        "global-require": "off"
      }
    }
  ]
}
